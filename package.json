{
  "name": "vpc",
  "version": "0.0.10",
  "main": "./dist/index.js",
  "autoInstall": false,
  "dependencies": {
    "@alicloud/pop-core": "^1.7.9",
    "@serverless-devs/core": "^0.0.x",
    "inquirer": "^7.3.3",
    "lodash": "^4.17.20",
    "promise-retry": "^2.0.1"
  },
  "devDependencies": {
    "@types/eslint": "^7.2.6",
    "@types/jest": "^26.0.10",
    "@types/lodash": "^4.14.168",
    "@types/node": "14",
    "f2elint": "^0.4.4",
    "jest": "^26.4.0",
    "rimraf": "^3.0.2",
    "ts-jest": "^26.2.0",
    "ts-loader": "^8.0.14",
    "typescript": "^3.9.7",
    "@vercel/ncc": "^0.24.0"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "npx tsc -w -p tsconfig.json",
    "prebuild": "rm -rf node_modules && rm -rf package-lock.json && npm i && rimraf dist",
    "build": "npm run fix && npm run lint && ncc build src/index.ts -m -e @serverless-devs/core -o dist",
    "test": "jest",
    "test:cov": "jest --coverage",
    "lint": "f2elint scan",
    "fix": "f2elint fix",
    "typecheck": "npx tsc -p tsconfig.json --noEmit"
  },
  "husky": {
    "hooks": {
      "commit-msg": "f2elint exec commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "f2elint exec lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts}": "f2elint exec eslint"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  }
}
